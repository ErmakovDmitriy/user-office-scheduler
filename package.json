{
  "name": "user-office-scheduler-backend",
  "version": "1.0.0",
  "description": "DUO-Backend-proto",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "prod": "tsc && env NODE_ENV=production node ./build/index.js",
    "dev": "env NODE_ENV=development ts-node-dev --respawn -T ./src/index.ts",
    "lint": "tsc --noEmit && eslint ./src --ext .js,.ts --quiet",
    "lint:fix": "tsc --noEmit && eslint ./src --ext .js,.ts --fix --quiet"
  },
  "lint-staged": {
    "**/*.{js,ts}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "SWAP",
  "license": "ISC",
  "dependencies": {
    "@apollo/federation": "^0.20.0",
    "@esss-swap/duo-localisation": "^1.1.12",
    "@esss-swap/duo-logger": "^1.0.3",
    "@esss-swap/duo-validation": "^1.1.30",
    "apollo-graphql": "^0.6.0",
    "apollo-server": "^2.17.0",
    "apollo-server-express": "^2.17.0",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0",
    "typescript": "^3.9.7",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.6",
    "prettier": "^1.19.1",
    "ts-node-dev": "^1.0.0-pre.62"
  }
